{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [false, "never"],
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-console": false,
    "max-classes-per-file": false,
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "rulesDirectory": []
}